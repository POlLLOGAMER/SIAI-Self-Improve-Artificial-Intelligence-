        "# â•°â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â•¯\n",
        "\n",
        "\n",
        "# â•­â”€â”€ UTILS â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â•®\n",
        "UA = {\"User-Agent\": \"Mozilla/5.0\"}\n",
        "def duck(q, k=5):\n",
        "    try:\n",
        "        h = requests.get(f\"https://duckduckgo.com/html/?q={requests.utils.quote(q)}\",\n",
        "                         headers=UA, timeout=10).text\n",
        "        s = bs4.BeautifulSoup(h, \"html.parser\")\n",
        "        return [n.get_text(\" \", strip=True) for n in s.select(\".result__snippet\")][:k]\n",
